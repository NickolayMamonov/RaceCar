{
  "Routes": [
    {
      "UseServiceDiscovery": true,
      "ServiceName": "race",
      "DownstreamPathTemplate": "/race",
      "DownstreamScheme": "https",      
      "UpstreamPathTemplate": "/race/",
      "UpstreamHttpMethod": [ "Get" ]
    }
  ],
   "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7057",
     "ServiceDiscoveryProvider": {
       "Host": "localhost",
       "Port": 8500,
       "Type": "PollConsul",
       "PollingInterval": 100
     }
   }
}